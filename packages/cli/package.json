{
  "name": "@viwo/cli",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "description": "CLI for VIWO - AI-powered development environment orchestrator",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["dist", "package.json", "README.md"],
  "bin": {
    "viwo": "./dist/cli.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "bun build src/index.ts src/cli.ts --outdir dist --target node --format esm && tsc --emitDeclarationOnly",
    "dev": "bun build src/index.ts src/cli.ts --outdir dist --target node --format esm --watch",
    "clean": "rm -rf dist *.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "test": "echo \"No tests yet\""
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@inquirer/prompts": "^8.0.1",
    "@viwo/core": "workspace:*",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
