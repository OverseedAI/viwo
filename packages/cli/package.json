{
  "name": "@viwo/cli",
  "version": "0.1.5",
  "private": false,
  "type": "module",
  "description": "CLI for VIWO - AI-powered development environment orchestrator",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "bin": {
    "viwo": "./dist/cli.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "bun build src/index.ts src/cli.ts --outdir dist --target node --format esm && tsc --emitDeclarationOnly",
    "build:binaries": "npm run build:linux-x64 && npm run build:linux-arm64 && npm run build:macos-x64 && npm run build:macos-arm64 && npm run build:windows-x64",
    "build:linux-x64": "bun build src/cli.ts --compile --target=bun-linux-x64 --outfile binaries/viwo-linux-x64",
    "build:linux-arm64": "bun build src/cli.ts --compile --target=bun-linux-arm64 --outfile binaries/viwo-linux-arm64",
    "build:macos-x64": "bun build src/cli.ts --compile --target=bun-darwin-x64 --outfile binaries/viwo-macos-x64",
    "build:macos-arm64": "bun build src/cli.ts --compile --target=bun-darwin-arm64 --outfile binaries/viwo-macos-arm64",
    "build:windows-x64": "bun build src/cli.ts --compile --target=bun-windows-x64 --outfile binaries/viwo-windows-x64.exe",
    "dev": "bun build src/index.ts src/cli.ts --outdir dist --target node --format esm --watch",
    "clean": "rm -rf dist *.tsbuildinfo binaries",
    "typecheck": "tsc --noEmit",
    "test": "echo \"No tests yet\""
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@inquirer/prompts": "^8.0.1",
    "@viwo/core": "workspace:*",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.5",
    "commander": "^11.1.0",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
